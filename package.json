{
  "name": "drizzle-orm-crsqlite-wasm",
  "version": "0.0.7",
  "description": "Drizzle ORM adapter for CR-SQLite WASM",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sheraff/drizzle-orm-crsqlite-wasm.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./migrator": {
      "import": "./dist/migrator.js",
      "types": "./dist/migrator.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "drizzle",
    "orm",
    "sqlite",
    "wasm",
    "crsqlite",
    "vlcn"
  ],
  "author": "@Sheraff",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@vlcn.io/xplat-api": "^0.15.0",
    "drizzle-orm": "^0.43.1"
  },
  "packageManager": "pnpm@10.12.2",
  "engines": {
    "node": ">=18.12"
  }
}
